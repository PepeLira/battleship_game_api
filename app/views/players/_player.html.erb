<div id="<%= dom_id player %>">
  <p>
    <strong>Name:</strong>
    <%= player.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= player.email %>
  </p>

  <p>
    <strong>Auth key:</strong>
    <%= player.auth_key %>
  </p>

  <p>
    <strong>N win games:</strong>
    <%= player.n_win_games %>
  </p>

  <p>
    <strong>N lose games:</strong>
    <%= player.n_lose_games %>
  </p>

  <p>
    <strong>N played games:</strong>
    <%= player.n_played_games %>
  </p>

  <p>
    <strong>N bonifications:</strong>
    <%= player.n_bonifications %>
  </p>

  <p>
    <strong>N effectiveness:</strong>
    <%= player.n_effectiveness %>
  </p>

  <p>
    <strong>Turns mean of games:</strong>
    <%= player.turns_mean_of_games %>
  </p>

  <p>
    <strong>Mean of misses by game:</strong>
    <%= player.mean_of_misses_by_game %>
  </p>

</div>
